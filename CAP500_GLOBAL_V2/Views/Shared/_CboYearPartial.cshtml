@Html.DevExpress().ComboBox(settings =>
{
    settings.Name = "CboYear";
    settings.ControlStyle.CssClass = "ComboCSS";
    settings.Properties.CallbackPageSize = 15;
    settings.Properties.TextField = "years";
    settings.Properties.ValueField = "id";
    settings.Properties.ClientSideEvents.SelectedIndexChanged = "OnCboYearChanged";
}).BindList(Model).GetHtml()

<script type="text/javascript">
    @{ var profileData = Session["DefaultGAINSess"] as GAIN.Models.LoginSession; }
    $(function () {
        CboYear.SetText(@profileData.ProjectYear);
    });
</script>